<% if node['td_agent']['includes'] %>
  include conf.d/*.conf
<% end %>
<% if node['td_agent']['default_config'] %>
  ## match tag=debug.** and dump to console
  <match debug.**>
    type stdout
  </match>

  <source>
    type forward
    port 24224
  </source>

  <source>
    type http
    port 8888
  </source>

  <source>
    type debug_agent
    bind 127.0.0.1
    port 24230
  </source>
<% end %>
